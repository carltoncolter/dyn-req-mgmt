    </ItemGroup>
    <Zip Files="@(ZipFiles)" ZipFileName="$(OutputPath)\PkgFolder\SampleData.zip" OverwriteExistingFile="True" />
  </Target>
  <Target Name="CopyPackagedSolution" AfterTargets="ZipSampleData" Condition="'$(Configuration)' == 'Debug' ">
